{
	"maxerr"   : 0,

	"quotmark" : "double",
	"indent"   : 4,
	"maxlen"   : 100,
	"maxdepth" : 4,

	"browser"  : true,
	"node"     : true,
	"devel"    : false,
	"esnext"   : true,

	"curly"    : true,
	"eqeqeq"   : true,
	"immed"    : true,
	"latedef"  : false,
	"laxbreak" : true,
	"newcap"   : true,
	"noarg"    : true,
	"noempty"  : true,
	"nonew"    : true,
	"plusplus" : false,
	"sub"      : true,
	"undef"    : true,
	"unused"   : true,

	"globals"  : {
		"Promise"        : true,
		"define"         : true,
		"requirejs"      : true,

		"asyncTest"      : true,
		"deepEqual"      : true,
		"equal"          : true,
		"expect"         : true,
		"module"         : true,
		"notDeepEqual"   : true,
		"notEqual"       : true,
		"notStrictEqual" : true,
		"ok"             : true,
		"QUnit"          : true,
		"raises"         : true,
		"start"          : true,
		"stop"           : true,
		"strictEqual"    : true,
		"test"           : true
	}
}