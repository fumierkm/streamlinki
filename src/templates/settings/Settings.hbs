<main class="content content-settings {{if isAnimated 'animated'}}">
<h2>Settings</h2>
<header>
<ul>
{{#each-in (hash
	main="Main"
	streams="Streams"
	livestreamer="Livestreamer"
	player="Player"
	chat="Chat"
	gui="GUI"
	lists="Lists"
	languages="Languages"
	notifications="Notifications"
	channels="Channels"
) as |subroute name|}}
	{{#link-to (concat "settings." subroute) tagName="li" classNames="btn" activeClass="btn-primary active" inactiveClass="btn-neutral"}}{{name}}{{/link-to}}
{{/each-in}}
</ul>
</header>
{{outlet}}
{{settings-submit isDirty=model.isDirty disabled=(is-equal currentSubmenu "settings.channels")}}
</main>