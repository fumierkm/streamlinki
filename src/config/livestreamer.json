{
	"exec": "livestreamer",
	"fallback": {
		"win32": [
			"%PROGRAMFILES%\\Livestreamer",
			"%PROGRAMFILES(X86)%\\Livestreamer"
		],
		"darwin": [
			"/usr/bin",
			"/usr/local/bin",
			"/Library/Frameworks/Python.framework/Versions/Current/bin"
		],
		"linux": [
			"/usr/bin",
			"/usr/local/bin"
		]
	},
	"version-min": {
		"livestreamer": "1.11.1",
		"streamlink": "0.0.1"
	},
	"validation-timeout": 10000,
	"download-url": "https://github.com/chrippa/livestreamer/releases",
	"docs-url": "http://docs.livestreamer.io/cli.html#cmdoption{item}"
}
