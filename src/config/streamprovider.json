{
	"default-provider": "streamlink",
	"validation-timeout": 10000,
	"version-min": {
		"livestreamer": "1.11.1",
		"streamlink": "0.0.1"
	},
	"download-url": {
		"livestreamer": "https://github.com/chrippa/livestreamer/releases",
		"streamlink": "https://github.com/streamlink/streamlink/releases"
	},
	"docs-url": {
		"livestreamer": "http://docs.livestreamer.io/cli.html#cmdoption{item}",
		"streamlink": "https://streamlink.github.io/cli.html#cmdoption{item}"
	},
	"providers": {
		"streamlink": {
			"type": "streamlink",
			"name": "Streamlink",
			"label": "Streamlink",
			"python": ">=2.6 <3 || >=3.3",
			"exec": {
				"win32": "pythonw.exe",
				"darwin": "python",
				"linux": "python"
			},
			"fallback": {
				"win32": [
					"%PROGRAMFILES%\\Streamlink\\Python",
					"%PROGRAMFILES(X86)%\\Streamlink\\Python"
				],
				"darwin": [
					"/usr/bin",
					"/usr/local/bin",
					"/Library/Frameworks/Python.framework/Versions/Current/bin"
				],
				"linux": [
					"/usr/bin",
					"/usr/local/bin"
				]
			},
			"pythonscript": {
				"win32": "streamlink-script.py",
				"darwin": "streamlink",
				"linux": "streamlink"
			},
			"pythonscriptfallback": {
				"win32": [
					"%PROGRAMFILES%\\Streamlink\\bin",
					"%PROGRAMFILES(X86)%\\Streamlink\\bin"
				],
				"darwin": [
					"/Library/Frameworks/Python.framework/Versions/Current/bin"
				],
				"linux": [
					"/usr/bin",
					"/usr/local/bin"
				]
			}
		},
		"livestreamer": {
			"type": "livestreamer",
			"name": "Livestreamer",
			"label": "Livestreamer",
			"python": ">=2.6 <3 || >=3.3",
			"exec": {
				"win32": "pythonw.exe",
				"darwin": "python",
				"linux": "python"
			},
			"fallback": {
				"win32": [],
				"darwin": [
					"/usr/bin",
					"/usr/local/bin",
					"/Library/Frameworks/Python.framework/Versions/Current/bin"
				],
				"linux": [
					"/usr/bin",
					"/usr/local/bin"
				]
			},
			"pythonscript": {
				"win32": "livestreamer-script.py",
				"darwin": "livestreamer",
				"linux": "livestreamer"
			},
			"pythonscriptfallback": {
				"win32": [],
				"darwin": [
					"/Library/Frameworks/Python.framework/Versions/Current/bin"
				],
				"linux": [
					"/usr/bin",
					"/usr/local/bin"
				]
			}
		},
		"livestreamer-standalone": {
			"type": "livestreamer",
			"name": "Livestreamer",
			"label": "Livestreamer (Standalone)",
			"exec": {
				"win32": "livestreamer.exe",
				"darwin": false,
				"linux": false
			},
			"fallback": {
				"win32": [
					"%PROGRAMFILES%\\Livestreamer",
					"%PROGRAMFILES(X86)%\\Livestreamer"
				]
			}
		}
	}
}
