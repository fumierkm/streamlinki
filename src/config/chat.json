{
	"browser": {
		"label": "Default browser"
	},
	"chromium": {
		"label": "Chromium",
		"exec": {
			"win32": "chrome.exe",
			"darwin": "Chromium",
			"linux": [
				"chromium",
				"chromium-browser",
				"chromium-dev",
				"chromium-continuous-bin",
				"chromium-snapshot-bin"
			]
		},
		"fallback": {
			"win32": [
				"%LOCALAPPDATA%\\Chromium\\Application",
				"%PROGRAMFILES%\\Chromium\\Application",
				"%PROGRAMFILES(X86)%\\Chromium\\Application"
			],
			"darwin": [
				"/Applications/Chromium.app/Contents/MacOS"
			],
			"linux": [
				"/usr/bin",
				"/usr/local/bin",
				"/opt/chromium"
			]
		},
		"attributes": [
			{
				"name": "exec",
				"title": "Chromium executable",
				"description": "Set a custom Chromium executable path.",
				"icon": "fa-cog",
				"type": "string",
				"isFile": true
			},
			{
				"name": "args",
				"title": "Chromium parameters",
				"description": "Set additional parameters.",
				"icon": "fa-terminal",
				"type": "string",
				"isArgs": true
			}
		]
	},
	"chrome": {
		"label": "Google Chrome",
		"exec": {
			"win32": "chrome.exe",
			"darwin": "Google Chrome",
			"linux": [
				"google-chrome",
				"google-chrome-stable",
				"google-chrome-unstable",
				"google-chrome-beta",
				"google-chrome-dev"
			]
		},
		"fallback": {
			"win32": [
				"%LOCALAPPDATA%\\Google\\Chrome\\Application",
				"%PROGRAMFILES%\\Google\\Chrome\\Application",
				"%PROGRAMFILES(X86)%\\Google\\Chrome\\Application"
			],
			"darwin": [
				"/Applications/Google Chrome.app/Contents/MacOS"
			],
			"linux": [
				"/usr/bin",
				"/usr/local/bin",
				"/opt/google/chrome",
				"/opt/google/chrome-unstable"
			]
		},
		"attributes": [
			{
				"name": "exec",
				"title": "Google Chrome executable",
				"description": "Set a custom Google Chrome executable path.",
				"icon": "fa-cog",
				"type": "string",
				"isFile": true
			},
			{
				"name": "args",
				"title": "Google Chrome parameters",
				"description": "Set additional parameters.",
				"icon": "fa-terminal",
				"type": "string",
				"isArgs": true
			}
		]
	},
	"msie": {
		"label": "Internet Explorer",
		"exec": {
			"win32": "cscript.exe",
			"darwin": false,
			"linux": false
		},
		"fallback": {
			"win32": [
				"%WINDIR%\\SysWOW64",
				"%WINDIR%\\system32"
			],
			"darwin": false,
			"linux": false
		},
		"data": {
			"script": "%NWJSAPPPATH%\\msie-minimal.vbs"
		}
	},
	"chatty": {
		"label": "Chatty",
		"exec": {
			"win32": "javaw.exe",
			"darwin": "java",
			"linux": "java"
		},
		"fallback": {
			"win32": [
				"%JAVA_HOME%\\bin",
				"%JRE_HOME%\\bin",
				"%PROGRAMDATA%\\Oracle\\Java\\javapath"
			],
			"darwin": [
				"$JAVA_HOME/bin",
				"$JRE_HOME/bin",
				"/usr/bin"
			],
			"linux": [
				"$JAVA_HOME/bin",
				"$JRE_HOME/bin",
				"/usr/bin",
				"/etc/alternatives"
			]
		},
		"jar": {
			"win32": "chatty.jar",
			"darwin": "chatty.jar",
			"linux": "chatty.jar"
		},
		"jarfallback": {
			"win32": [],
			"darwin": [],
			"linux": [
				"/usr/share/chatty"
			]
		},
		"attributes": [
			{
				"name": "exec",
				"title": "Java executable",
				"description": "Set a custom java executable path.",
				"icon": "fa-cog",
				"type": "string",
				"isFile": true
			},
			{
				"name": "jar",
				"title": "Chatty .jar file",
				"description": "Set a custom chatty.jar path.",
				"icon": "fa-cog",
				"type": "string",
				"isFile": true
			},
			{
				"name": "instance",
				"title": "Single instance mode",
				"description": "Requires at least version 0.8.2b2.",
				"icon": "fa-clone",
				"type": "boolean",
				"defaultValue": true,
				"text": "Enable single instance mode"
			},
			{
				"name": "auth",
				"title": "Authenticate",
				"description": "Let Chatty re-use the auth data.",
				"icon": "fa-sign-in",
				"type": "boolean",
				"defaultValue": true,
				"text": "Sign in automatically"
			},
			{
				"name": "args",
				"title": "Custom Chatty parameters",
				"description": "Set additional parameters.",
				"icon": "fa-terminal",
				"type": "string",
				"isArgs": true
			}
		]
	},
	"custom": {
		"label": "Custom application",
		"attributes": [
			{
				"name": "exec",
				"title": "Application executable",
				"description": "Set the path to the executable.",
				"icon": "fa-cog",
				"type": "string",
				"isFile": true
			},
			{
				"name": "args",
				"title": "Application parameters",
				"description": "Set application parameters.",
				"icon": "fa-terminal",
				"type": "string",
				"isArgs": true
			}
		]
	}
}
